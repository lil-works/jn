{% extends 'baseAdmin.html.twig' %}

{% block body %}
    <h1>Instrumentfamily</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ instrumentFamily.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ instrumentFamily.name }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ instrumentFamily.icon }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('instrumentfamily_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('instrumentfamily_edit', { 'id': instrumentFamily.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
