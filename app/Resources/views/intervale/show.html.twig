{% extends 'base.html.twig' %}

{% block body %}
    <h1>Intervale</h1>
    <ul class="contentMenu">
        <li>
            <a href="{{ path('intervale_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('intervale_edit', { 'id': intervale.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ intervale.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ intervale.name }}</td>
            </tr>
            <tr>
                <th>Delta</th>
                <td>{{ intervale.delta }}</td>
            </tr>
            <tr>
                <th>Color</th>
                <td>{{ intervale.color }}</td>
            </tr>
        </tbody>
    </table>


{% endblock %}
