{% extends 'baseAdmin.html.twig' %}

{% block body %}
    <h1>Rhythmsequence</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ rhythmSequence.id }}</td>
            </tr>
            <tr>
                <th>Signaturenominator</th>
                <td>{{ rhythmSequence.signatureNominator }}</td>
            </tr>
            <tr>
                <th>Signaturedenominator</th>
                <td>{{ rhythmSequence.signatureDenominator }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ rhythmSequence.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ rhythmSequence.description }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('rhythmsequence_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('rhythmsequence_edit', { 'id': rhythmSequence.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
