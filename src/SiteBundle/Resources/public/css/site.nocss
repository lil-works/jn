 }
.quote{
    color: rgba(0,0,0,.2);
    font-weight: 100;
    padding: 20px;
    font-weight: 100;
    font-size: 2.2em;
    text-shadow: 0.5px 0.5px 0.5px rgba(0,0,0,.2);
}
.quote:before{
    content: "\201C";
}
.quote:after{
    content: "\201D";
}
.contentMenu>li{
