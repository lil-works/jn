*{
    margin: 0;
    padding: 0;
    -moz-box-sizing: border-box; box-sizing: border-box;
}
html , body{
    height: 100%;
}

body {
    position: relative;
    min-height: 100%;
    background-color: #fff;
    font-family: 'Rubik', Andale Mono, monospace;
    font-size: 12px;
    letter-spacing: 1.6px;

}
#loading {
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    position: fixed;
    display: block;
    opacity: 0.5;
    background-color: #fff;
    z-index: 99;
    text-align: center;
}
#loading p{font-size: 2em;margin: 10px;padding: 10px;}

canvas{cursor:pointer;}
h1,h2,h3,h4,h5,h6{

    overflow: hidden;
    letter-spacing: .3em;
    margin:15px;
    font-weight: 100;
    text-shadow: 0.5px 0.5px 0.5px #000;
}
a{
    color: #0088CC;
    font-weight: 100;
    text-decoration: none;

}
a:hover{
    color: #0077BB;
    font-weight: 300;

}

.wrapper {
    min-height: 100%;
    height: auto !important; /* This line and the next line are not necessary unless you need IE6 support */
    height: 100%;
    margin: 0 auto -40px; /* the bottom margin is the negative value of the footer's height */
    background: repeating-linear-gradient(
            -45deg,
            #fbfbfb,
            #fefefe 3px,
            #fefefe 4px,
            #ffffff 8px
    )
}
#footer, .push {
    height: 40px; /* .push must be the same height as .footer */

}
#footer{
    text-shadow: 1px 1px 1px #ccc;
    color: #ffffff;
    padding: 8px;
    text-align: right;
    width:100%;
    background-image:
    linear-gradient(
            to top,
            #eee, #fefefe
    );
    border-top: 1px solid #eee;

}
#footer>img{

height: 30px;
    float: left;

}
#menu{float: right;}
#menu > ul > li > a{
    font-size: 2em;
    letter-spacing: 5px;

}
#menu > ul > li:after {
    content: " • ";
}
#menu>ul>li{display: inline;list-style: none;}
#header{
    width:100%;
    margin:  auto;

    background-image:
    linear-gradient(
            to bottom,
            #eee, #fefefe
    );
    border-bottom: 1px solid #eee;

}
#menu{
margin: 10px;
    padding: 10px;
}
#logo{
    margin: 5px;
    height:80px;

}
#content{

    width:1280px;
    margin:auto auto;
    padding-top: 22px ;

}

#content h1{
    color: rgba(0,0,0,.1);
    font-size: 2em;
    font-weight: 100;
    letter-spacing: .3em;
    margin:5px;
    text-align: right;
    text-shadow: 1px 1px 1px #000;

}
#content h2{
    color: rgba(0,0,0,.1);
    font-size: 1.6em;
    font-weight: 100;

}

.form_row{
    margin:5px;
    padding:5px;
}
.form_row > label{
    display: block;
    color: #565656;
}
.form_row > select , input{
    border: 1px solid #ccc ;
    border-radius: 2px ;
    margin: 5px;
    padding: 5px;
    font-size:1em;
}
.form_row > select >option{
    display: inline;
    margin: 3px;
    padding: 3px;
}
table{
    border-radius: 4px;
    background: #fff none repeat scroll 0 0;
    box-shadow: 0 0 1em #f0f0f0 inset, 0 0 1em #f0f0f0;
    margin: 10px;
    padding: 10px;
    width:100%;

}
table>thead{

}
table>tbody{

}
table>thead>tr{
    margin: 5px;
    padding: 5px;
}
table>thead>tr>td{
    margin: 5px;
    padding: 5px;
}
.intervale{
    font-size: 1.2em;
    font-weight: bold;
    border: 1px #ccc solid;
    border-radius: 2px;
    padding: 2px;
    margin: 2px;
    background-color: #f0f0f0;
}


div.inline { float:left; }
.clearBoth { clear:both; }

.textButton{
    color: #0088CC;
    font-size: 1em;
    font-weight: bolder;
    cursor: pointer;
}
#logo{



}
#neckContainer{
    padding: 14px;
    margin:35px;
    text-align: center;
    border-radius: 4px;
    background: #fff none repeat scroll 0 0;
    box-shadow: 0 0 1em #f0f0f0 inset, 0 0 1em #f0f0f0;
}
#controls div>*{margin: 0;}
#controls div>h3{margin-top: -5px;margin-bottom: 5px;}
#controls>div{
    margin: 14px;
    width:400px;
    border-radius: 4px;
    background: #f5f5f5 none repeat scroll 0 0;
    box-shadow: 0 0 1em #f0f0f0 inset, 0 0 1em #f0f0f0;
}
#controls>div>ul>li{
    list-style: none;
}

#neckResultsList>li {
    width:210px;
    height:210px;
    text-align: center;
    border-radius: 2px;
padding: 10px;
    margin:auto;

    display: inline-block;
    background-color: #fff;
    box-shadow: 0 0 .5em #f0f0f0 inset, 0 0 1em #f0f0f0;
}
#neckResultsList>li>div {

}
